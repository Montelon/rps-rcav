<h1><%=@some_number%> </h1> 

<h1>Hello World</h1>

<%= index = rand(3) %>

<% moves = ["rock", "paper", "scissor"]%>

<% comp_move = moves.at(index) %>
<% if comp_move == "scissor"%>
  <%=outcome="won"%>

  <% elsif comp_move=="rock"%>
  <%=outcome="tied"%>
  <%elsif comp_move="paper"%>
  <%outcome="lost"%>
  <%end%>


<h2> We played rock!</h2>

<h2> They played <%= comp_move%>!</h2>
<h2> We <%=outcome%></h2>
